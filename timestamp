def timestamp():
    from datetime import datetime
    now = datetime.now()
    #print(now)

    #timestamp = now.strftime("%x") + " " + now.strftime("%X")


    #print(timestamp)

    #timestamp1 = now.strftime("%x") + " " + now.strftime("%H:%M")
    #print(timestamp1)


    ts = now.strftime("%y_%m_%d_%H_%M_%S")
    return ts

